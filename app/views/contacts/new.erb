<h1>Contacts#new</h1>
<p>Find me in app/views/contacts/new.html.erb</p>

<%= form_for @contact, url: { action: 'create'} do |f| %>
  <%= f.label :name %>
  <%= f.text_field :name %>
  <br>
  <%= f.label :role %>
  <%= f.text_field :role %>
  <br>
  <%= f.label :email %>
  <%= f.text_field :email %>
  <br>
  <%= f.label :phone %>
  <%= f.text_field :phone %>
  <br>
  <%= f.label :decision_maker %>
  <%= f.label 'Yes' %>
  <%= f.radio_button :decision_maker, true %>
  <%= f.label 'No' %>
  <%= f.radio_button :decision_maker, false %>
  <br>
  <%= f.submit 'Create Contact' %>
<% end %>
